{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "hp",
    "version": "0.1.0",
    "identifier": "com.ax.hp",
    "build": {
        "frontendDist": "../src"
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [
            {
                "title": "hp",
                "width": 1400,
                "height": 900,
                "transparent": true,
                "decorations": false,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
        ]
    }
}
